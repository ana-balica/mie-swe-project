@prefix rdf:		 <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dbpedia-owl: <http://dbpedia.org/ontology/> .
@prefix dc:			 <http://purl.org/dc/elements/1.1/>
@prefix geo:		 <http://www.w3.org/2003/01/geo/wgs84_pos#>
@prefix sd:			 <http://www.self-defined.org/>


{% for airport_name, airport_data in airports.iteritems() %}
sd:airport#{{ airport_name }} a dbpedia-owl:Airport
{%- for key, value in airport_data.iteritems() -%}
{% if key == "icao" %}
sd:airport#{{ airport_name }} pr:identifier {{ value }} .
{% elif key == "country" -%}
sd:coutry#{{ value }} a dbpedia-owl:country .
sd:airport#{{ airport_name }} pr:coverage sd:coutry#{{ value }} .
{% elif key == "faa" -%}
sd:location#{{ value }} rdf:domain dbpedia-owl:faaLocationIdentifier .
sd:airport#{{ airport_name }} pr:coverage sd:location#{{ value }} .
{% elif key == "lat" -%}
sd:airport#{{ airport_name }} geo:latitude {{ value }} .
{% elif key == "long" -%}
sd:airport#{{ airport_name }} geo:longitude {{ value }} .
{% elif key == "alt" -%}
sd:airport#{{ airport_name }} geo:altitude {{ value }} .
{% endif -%}
{% endfor -%}
{% endfor -%}